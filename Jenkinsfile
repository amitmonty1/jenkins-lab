pipeline{
    agent any
    stages{
        stage("get source code"){
            steps{
                echo "getting file from github"
            }
        }
        stage("build"){
            steps{
                echo "buiding the project"
            }
        }
        stage("deploy"){
            steps{
                echo "Deploying the project"
            }
        }     
        stage("test"){
            steps{
                echo "Testing the project"
            }
        }             
    }
}